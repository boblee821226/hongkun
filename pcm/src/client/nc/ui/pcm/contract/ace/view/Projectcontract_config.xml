<?xml version="1.0" encoding="gbk"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="classpath:nc/ui/pmpub/config/action_config.xml" />
	<import resource="classpath:nc/ui/pmpub/config/frame_config.xml" />
	<import resource="classpath:nc/ui/pm/config/pagebar_config.xml" />

	<!-- 环境变量 -->
	<bean id="context" class="nc.vo.uif2.LoginContext" />


	<!-- 对象转换器工厂，由此获取操作对象的特征信息 -->
	<bean id="boadatorfactory"
		class="nc.ui.pubapp.uif2app.view.value.AggVOMetaBDObjectAdapterFactory" />

	<!-- 管理应用模型 -->
	<bean id="manageAppModel" class="nc.ui.pmpub.model.BillManageModel"
		init-method="initModelContext">
		<property name="businessObjectAdapterFactory" ref="boadatorfactory"></property>
		<property name="context" ref="context"></property>
		<property name="billType" ref="billType"></property>
		<property name="last_v_flag" value="last_v_flag"></property>
	</bean>

	<bean id="billType" class="nc.ui.pm.config.VarString">
		<property name="valueStr" value="4D42" />
	</bean>

	<!-- 数据模型管理器，主要负责各种方式的模型初始化 -->
	<bean id="modelDataManager" class="nc.ui.pm.model.PMPaginationModelDataManager">
		<property name="model" ref="manageAppModel"></property>
		<property name="paginationModel" ref="paginationModel" />
		<property name="pageQueryService" ref="pageQueryService" />
	</bean>

	<!-- 模板容器，负责对模板的统一装载 -->
	<bean id="templateContainer" class="nc.ui.uif2.editor.TemplateContainer">
		<property name="context" ref="context" />
	</bean>

	<!-- 查询模板加载bean -->
	<bean id="queryTemplateContainer" class="nc.ui.uif2.editor.QueryTemplateContainer">
		<property name="context" ref="context"></property>
	</bean>

	<!-- 界面元素 -->
	<!-- 列表视图 -->
	<bean id="listView" class="nc.ui.pmpub.view.PMBillListView"
		init-method="initUI">
		<property name="model" ref="manageAppModel"></property>
		<property name="paginationBar" ref="paginationBar" />
		<property name="templateContainer" ref="templateContainer"></property>

		<!--<property name="showTotalLine" value="true"></property> <property name="showTotalLineTabcodes"> 
			<list> <value>works</value> </list> </property> --><!-- 列表自定义项处理 -->
		<property name="userdefitemListPreparator">
			<bean class="nc.ui.pubapp.uif2app.view.CompositeBillListDataPrepare">
				<property name="billListDataPrepares">
					<list>
						<ref bean="userdefitemlistPreparator" />
						<!-- <ref bean="marAsstPreparator" /> -->
					</list>
				</property>
			</bean>
		</property>
	</bean>

	<!-- 列表自定义项处理 -->
	<bean name="userdefitemlistPreparator" class="nc.ui.uif2.editor.UserdefitemContainerListPreparator">
		<property name="container" ref="userdefitemContainer" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.contractheadvo" />
					<property name="pos" value="0" />
					<property name="prefix" value="hdef" />
					<property name="rulecode" value="4D42_H" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.works" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="works" />
					<property name="rulecode" value="4D42_W" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.items" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="items" />
					<property name="rulecode" value="4D42_I" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.history" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="history" />
					<property name="rulecode" value="4D42_A" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.exepro" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="exepro" />
					<property name="rulecode" value="4D42_E" />
				</bean>
			</list>
		</property>
	</bean>


	<!-- 卡片自定义项配置 -->
	<bean name="userdefitemPreparator" class="nc.ui.uif2.editor.UserdefitemContainerPreparator">
		<property name="container" ref="userdefitemContainer" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.contractheadvo" />
					<property name="pos" value="0" />
					<property name="prefix" value="hdef" />
					<property name="rulecode" value="4D42_H" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.works" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="works" />
					<property name="rulecode" value="4D42_W" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.items" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="items" />
					<property name="rulecode" value="4D42_I" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.history" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="history" />
					<property name="rulecode" value="4D42_A" />
				</bean>
				<bean class="nc.ui.uif2.editor.UserdefQueryParam">
					<property name="mdfullname" value="pcm.exepro" />
					<property name="pos" value="1" />
					<property name="prefix" value="bdef" />
					<property name="tabcode" value="exepro" />
					<property name="rulecode" value="4D42_E" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="userdefitemContainer" class="nc.ui.uif2.userdefitem.UserDefItemContainer">
		<property name="context" ref="context" />
		<property name="params">
			<list>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pcm.contractheadvo" />
					<property name="rulecode" value="4D42_H" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pcm.works" />
					<property name="rulecode" value="4D42_W" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pcm.items" />
					<property name="rulecode" value="4D42_I" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pcm.history" />
					<property name="rulecode" value="4D42_A" />
				</bean>
				<bean class="nc.ui.uif2.userdefitem.QueryParam">
					<property name="mdfullname" value="pcm.exepro" />
					<property name="rulecode" value="4D42_E" />
				</bean>
			</list>
		</property>
	</bean>

	<bean id="billFormEditor" class="nc.ui.pmpub.view.PMBillFormForProject"
		init-method="initUI">
		<property name="model" ref="manageAppModel"></property>
		<property name="templateContainer" ref="templateContainer" />

		<property name="showOrgPanel" value="true" />
		<!-- 是否自动增行 -->
		<property name="autoAddLine" value="true" />
		<property name="allAutoAddLine" value="true" />
		<!-- 空行过滤器，卡片界面根据此过滤器删除表体空行 -->
		<property name="blankChildrenFilter">
			<!-- 简单的根据一个字段是否为空的空行过滤器，因为多数情况下都是判断存货是否为空来过滤空行 -->
			<bean
				class="nc.ui.pubapp.uif2app.view.value.MultiFieldsBlankChildrenFilter">
				<property name="nullAssertByOr" value="false" />
				<property name="filterMap">
					<ref bean="noBlankRowKeyFiledsMap" />
				</property>
			</bean>
		</property>

		<property name="showTotalLineTabcodes">
			<list>
				<value>works</value>
			</list>
		</property>

		<!-- 表体行操作按钮注册，与表体右键菜单保持一致 -->
		<property name="bodyActionMap">
			<map>
				<entry key="works">
					<list>
						<!-- 提供了一组默认实现 -->
						<!-- 提供了一组默认实现 -->
						<bean class="nc.ui.pcm.contract.action.AddLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyInsertLineAction" />
						<bean class="nc.ui.pcm.contract.action.DelLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyLineEditAction" />
						<bean class="nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
						<bean class="nc.ui.pm.action.BillViewMaxAction">
							<property name="billForm" ref="billFormEditor" />
							<property name="pos" value="1" />
						</bean>
					</list>
				</entry>

				<entry key="items">
					<list>
						<!-- 提供了一组默认实现 -->
						<!-- 提供了一组默认实现 -->
						<bean class="nc.ui.pubapp.uif2app.actions.BodyAddLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyInsertLineAction" />
						<bean class="nc.ui.pubapp.uif2app.actions.BodyDelLineAction" />
						<!-- <bean class="nc.ui.pubapp.uif2app.actions.BodyLineEditAction" 
							/> -->
						<bean class="nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
						<bean class="nc.ui.pm.action.BillViewMaxAction">
							<property name="billForm" ref="billFormEditor" />
							<property name="pos" value="1" />
						</bean>
					</list>
				</entry>

				<entry key="history">
					<list>
						<!-- 提供了一组默认实现 -->
						<bean class="nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
						<bean class="nc.ui.pm.action.BillViewMaxAction">
							<property name="billForm" ref="billFormEditor" />
							<property name="pos" value="1" />
						</bean>
					</list>
				</entry>

				<entry key="exepro">
					<list>
						<!-- 提供了一组默认实现 -->
						<bean class="nc.ui.pub.beans.ActionsBar$ActionsBarSeparator" />
						<bean class="nc.ui.pm.action.BillViewMaxAction">
							<property name="billForm" ref="billFormEditor" />
							<property name="pos" value="1" />
						</bean>
					</list>
				</entry>
			</map>
		</property>

		<!-- 卡片自定义项处理 -->
		<property name="userdefitemPreparator">
			<bean class="nc.ui.pubapp.uif2app.view.CompositeBillDataPrepare">
				<property name="billDataPrepares">
					<list>
						<ref bean="userdefitemPreparator" />
						<!-- <ref bean="marAsstPreparator" /> -->
					</list>
				</property>
			</bean>
		</property>

	</bean>
	<!-- 判断依据：列出的字段中有一个不为空，则不是空行 -->
	<bean id="noBlankRowKeyFiledsMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry>
					<!--页签code -->
					<key>
						<value>works</value>
					</key>
					<!--检验是否空行字段code -->
					<list>
						<value>pk_material</value>
					</list>
				</entry>

				<entry>
					<!--页签code 合同条款的条款编码和条款内容都为空，则视为空行过滤掉 -->
					<key>
						<value>items</value>
					</key>
					<!--检验是否空行字段code -->
					<list>
						<value>pk_ct_termset</value>
						<value>vtermcontent</value>
					</list>
				</entry>

				<entry>
					<!--页签code -->
					<key>
						<value>history</value>
					</key>
					<!--检验是否空行字段code -->
					<list>
						<value>contr_v</value>
					</list>
				</entry>
				<entry>
					<!--页签code -->
					<key>
						<value>exepro</value>
					</key>
					<!--检验是否空行字段code -->
					<list>
						<value>exe_flow</value>
					</list>
				</entry>
			</map>
		</constructor-arg>
	</bean>

	<!-- 自由辅助属性的配置 -->
	<bean id="marAsstPreparator"
		class="nc.ui.pubapp.uif2app.view.material.assistant.MarAsstPreparator">
		<property name="model" ref="manageAppModel" />
		<property name="container" ref="userdefitemContainer" />
		<property name="prefix" value="vfree" />
	</bean>

	<!-- 快速查询区域 -->
	<bean id="queryArea" class="nc.ui.pubapp.uif2app.tangramlayout.UEQueryAreaShell"
		init-method="initUI">
		<property name="queryAreaCreator" ref="queryAction" />
	</bean>
	<!-- 查询信息栏 -->
	<bean id="queryInfo" class="nc.ui.uif2.tangramlayout.CardLayoutToolbarPanel">
		<property name="model" ref="manageAppModel" />
	</bean>

	<bean id="pmAttachmentActionListener" class="nc.ui.pm.action.PMAttachmentActionListener">
		<property name="model" ref="manageAppModel" />
		<!-- 审批后不能删除 -->
		<property name="approveNoDelete" value="true"></property>
	</bean>
	<!-- 模型服务 -->
	<bean id="modelService" class="nc.ui.pmpub.model.AppModelService">
		<property name="context" ref="context" />
	</bean>

	<!-- 界面布局总装 -->
	<bean id="container" class="nc.ui.uif2.TangramContainer"
		init-method="initUI">
		<property name="model" ref="manageAppModel" />
		<property name="tangramLayoutRoot">
			<bean class="nc.ui.uif2.tangramlayout.node.TBNode">
				<property name="showMode" value="CardLayout" />
				<property name="tabs">
					<list>
						<bean class="nc.ui.uif2.tangramlayout.node.HSNode">
							<property name="left">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="component" ref="queryArea" />
								</bean>
							</property>
							<property name="right">
								<bean class="nc.ui.uif2.tangramlayout.node.VSNode">
									<property name="up">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="component" ref="queryInfo" />
										</bean>
									</property>
									<property name="down">
										<bean class="nc.ui.uif2.tangramlayout.node.CNode">
											<property name="name" value="列表" />
											<property name="component" ref="listView" />
										</bean>
									</property>
									<property name="dividerLocation" value="25f" />
								</bean>
							</property>
							<property name="dividerLocation" value="210f" />
						</bean>
						<bean class="nc.ui.uif2.tangramlayout.node.VSNode">
							<property name="up">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="component" ref="cardInfoPnl" />
								</bean>
							</property>
							<property name="down">
								<bean class="nc.ui.uif2.tangramlayout.node.CNode">
									<property name="name" value="卡片" />
									<property name="component" ref="billFormEditor" />
								</bean>
							</property>
							<property name="dividerLocation" value="30f" />
						</bean>
					</list>
				</property>
			</bean>
		</property>
	</bean>

	<!-- 事件监听 -->
	<bean id="childrenPicky" class="nc.ui.pubapp.uif2app.event.ChildrenPicky">
		<property name="billform" ref="billFormEditor" />
		<property name="bodyVoClasses">
			<list>
				<value>nc.vo.pcm.contract.ContrWorksVO</value>
				<value>nc.vo.pcm.contract.ContrItemsVO</value>
				<value>nc.vo.pcm.contract.ContrExeProVO</value>
			</list>
		</property>
	</bean>
	<bean id="eventMediator" class="nc.ui.pubapp.uif2app.model.AppEventHandlerMediator">
		<property name="handlerGroup">
			<list>
				<!-- 表体字段编辑前事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardBodyBeforeEditEvent" />
					<property name="picky" ref="childrenPicky" />
					<property name="handler">
						<bean class="nc.ui.pcm.contract.ace.handler.AceBodyBeforeEditHandler" />
					</property>
				</bean>

				<!-- 表体字段编辑后事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardBodyAfterEditEvent" />
					<property name="picky" ref="childrenPicky" />
					<property name="handler">
						<bean class="nc.ui.pcm.contract.ace.handler.AceBodyAfterEditHandler" />
					</property>
				</bean>

				<!-- 表头表尾字段编辑前事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardHeadTailBeforeEditEvent" />
					<property name="handler">
						<bean
							class="nc.ui.pcm.contract.ace.handler.AceHeadTailBeforeEditHandler" />
					</property>
				</bean>

				<!-- 表头表尾字段编辑后事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.card.CardHeadTailAfterEditEvent" />
					<property name="handler">
						<bean class="nc.ui.pcm.contract.ace.handler.AceHeadTailAfterEditHandler" />
					</property>
				</bean>

				<!-- 新增事件 -->
				<bean class="nc.ui.pubapp.uif2app.event.EventHandlerGroup">
					<property name="event"
						value="nc.ui.pubapp.uif2app.event.billform.AddEvent" />
					<property name="handler">
						<bean class="nc.ui.pcm.contract.ace.handler.AceAddHandler" />
					</property>
				</bean>

			</list>
		</property>

		<property name="handlerMap">
			<map>
				<!-- 表头字段编辑后事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardHeadTailAfterEditEvent">
					<list>
						<ref bean="HeadAfterEditHandler" />
						<ref bean="ProjectSBSHeadAfterEditHandler" />
						<ref bean="BeginFlagAfterEditHandler" />
						<ref bean="psnAfterEditHandler" />
					</list>
				</entry>

				<!-- 表头字段编辑前事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardHeadTailBeforeEditEvent">
					<list>
						<ref bean="HeadBeforeEditHandler" />
						<bean class="nc.ui.pm.disposeprojectref.HeadProjectRefBeforeEditHandler" />
						<ref bean="pk_currtypeBeforeHandler" />
						<ref bean="pk_projectBeforeHandler" />
						<ref bean="BeginFlagBeforeEditHandler" />
						<ref bean="deptAndPsnBeforeEditHandler" />
					</list>
				</entry>

				<!-- 组织切换事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.OrgChangedEvent">
					<list>
						<ref bean="orgChangedHander" />
					</list>
				</entry>

				<!-- 表体字段编辑后事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyAfterEditEvent">
					<list>
						<ref bean="BodyAfterEditHandler" />
					</list>
				</entry>

				<!-- 表体字段编辑前事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.card.CardBodyBeforeEditEvent">
					<list>
						<ref bean="BodyBeforeEditHandler" />
						<ref bean="CBSFilterBody" />
					</list>
				</entry>

				<!-- 列表界面表头行切换事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.list.ListHeadRowChangedEvent">
					<list>
						<ref bean="list_headrow_chghandler" />
					</list>
				</entry>

				<!-- 列表加载数据事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.list.ListPanelLoadEvent">
					<list>
						<ref bean="list_loadhandler" />
					</list>
				</entry>

				<!-- 新增事件 -->
				<entry key="nc.ui.pubapp.uif2app.event.billform.AddEvent">
					<list>
						<ref bean="add_handler" />
					</list>
				</entry>
			</map>
		</property>
		<property name="model" ref="manageAppModel" />
	</bean>
	<!-- 跨组织选部门事件 -->
	<bean id="deptAndPsnBeforeEditHandler" class="nc.ui.pm.handler.DeptAndPsnBeforeEditHandler">
		<property name="pk_dept" value="pk_pro_dept_v" />
		<property name="pk_psn" value="pk_promisepsn" />
		<property name="cardForm" ref="billFormEditor" />
	</bean>

	<bean id="psnAfterEditHandler" class="nc.ui.pm.handler.PsnAfterEditHandler">
		<property name="pk_dept" value="pk_promise_dept" />
		<property name="pk_psn" value="pk_promisepsn" />
		<property name="cardForm" ref="billFormEditor" />
	</bean>
	<bean id="scaleProcess" class="nc.ui.pcm.contract.util.ContractScaleUtil"></bean>
	<!--表体 中CBS过滤 -->
	<bean id="CBSFilterBody" class="nc.ui.pm.handler.CBSNodeBodyBeforeEditHandler">
		<property name="CBSNodeFiled" value="pk_cbsnode" />
		<property name="projectIDFeild" value="pk_project" />
	</bean>
	<!-- 组织切换后处理 -->
